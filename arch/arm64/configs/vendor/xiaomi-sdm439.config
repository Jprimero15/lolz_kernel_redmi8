# Localversion
CONFIG_LOCALVERSION_AUTO=y

# CPU
CONFIG_CPU_FREQ_STAT=y
CONFIG_CPU_FREQ_TIMES=n
CONFIG_CPU_BOOST=n
CONFIG_MSM_PERFORMANCE=n
CONFIG_CPUSET_ASSIST=y
CONFIG_CPUSET_AUDIO_APP="6-7"
CONFIG_CPUSET_BG="4-5"
CONFIG_CPUSET_CAMERA="0-7"
CONFIG_CPUSET_DISPLAY="0-3"
CONFIG_CPUSET_FG="0-7"
CONFIG_CPUSET_RESTRICTED="4-7"
CONFIG_CPUSET_SYSTEM_BG="4-7"

# Crypto
CONFIG_CRYPTO_LZO=y
CONFIG_CRYPTO_LZ4=y
CONFIG_CRYPTO_ZSTD=y

# HZ
CONFIG_HZ_300=y

# Device mapper
CONFIG_DM_ANDROID_VERITY=n

# DCE
CONFIG_LD_DEAD_CODE_DATA_ELIMINATION=y

# ION
CONFIG_ION_LEGACY=y

# Netfilter
CONFIG_IP_NF_TARGET_TTL=y

# OF
CONFIG_BUILD_ARM64_APPENDED_DTB_IMAGE=y
CONFIG_OF_IGNORE_QCOM_VENDOR_DTS=y
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# Panic
CONFIG_PANIC_TIMEOUT=1

# Pstore
CONFIG_PSTORE=y
CONFIG_PSTORE_LZ4_COMPRESS=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_RAM=y

# Ramdisk
CONFIG_RD_LZMA=y
CONFIG_RD_XZ=y
CONFIG_INITRAMFS_IGNORE_SKIP_FLAG=y

# Reduce size
CONFIG_SLUB_DEBUG=n
CONFIG_IKHEADERS=n
CONFIG_MODULES=n

# Suspend
CONFIG_SUSPEND_SKIP_SYNC=y

# Userdata casefolding
CONFIG_UNICODE=y
CONFIG_SDCARD_FS=n

# vDSO
CONFIG_ARM_ARCH_TIMER_VCT_ACCESS=y
CONFIG_COMPAT_VDSO=y

# WLAN
CONFIG_PRONTO_WLAN=y

# zRAM
CONFIG_ZRAM=y
# CONFIG_ZRAM_DEDUP is not set
CONFIG_ZRAM_DEF_COMP_LZ4=y

# Debug
CONFIG_MSM_IDLE_STATS=n
CONFIG_PAGE_EXTENSION=n
CONFIG_PAGE_OWNER=n
CONFIG_DEBUG_FS=n
CONFIG_IPC_LOGGING=n

# MSM CLK
CONFIG_QCOM_CLK_SMD_RPM=n
CONFIG_SDM_GCC_429W=n
CONFIG_SDM_DEBUGCC_429W=n
CONFIG_CLOCK_CPU_SDM=n
CONFIG_SDM_DEBUGCC_439=n
CONFIG_MSM_MDSS_PLL=y

# Sched
CONFIG_RT_GROUP_SCHED=n
CONFIG_SCHED_AUTOGROUP=n
CONFIG_CGROUPS=y
CONFIG_CGROUP_SCHED=y
CONFIG_CGROUP_FREEZER=y
CONFIG_FAIR_GROUP_SCHED=n
CONFIG_SCHED_CORE_CTL=n
CONFIG_SCHED_WALT=n
CONFIG_UCLAMP_TASK=n
CONFIG_PELT_UTIL_HALFLIFE_16=y
CONFIG_PELT_COMPATIBILITY_LAYER=y
CONFIG_SCHED_CASS=y
CONFIG_STUNE_ASSIST=y
CONFIG_SCHEDUTIL_UP_RATE_LIMIT=0
CONFIG_SCHEDUTIL_DOWN_RATE_LIMIT=0
CONFIG_CFQ_GROUP_IOSCHED=y

# SMP2P
CONFIG_QCOM_SMP2P_SLEEPSTATE=y

# Kernel target level 5 -> 6
CONFIG_ANDROID_PARANOID_NETWORK=n
CONFIG_CRYPTO_CHACHA20POLY1305=y
CONFIG_CRYPTO_CMAC=y
CONFIG_CRYPTO_CTR=y
CONFIG_CRYPTO_XCBC=y
CONFIG_SONY_FF=y
CONFIG_GKI_HIDDEN_GPU_CONFIGS=y
CONFIG_TRACE_GPU_MEM=y
CONFIG_XFRM_MIGRATE=y

# F2FS
CONFIG_F2FS_FS_COMPRESSION=y

# Exfat
CONFIG_EXFAT_FS=y

# NTFS
CONFIG_NTFS_FS=y
CONFIG_NTFS_RW=y

# UFS
CONFIG_SCSI_UFSHCD=n

# LMKD
CONFIG_PSI=y
CONFIG_MEMCG=y
CONFIG_MEMCG_SWAP=y
CONFIG_ANDROID_LOW_MEMORY_KILLER=n

# Wireguard
CONFIG_WIREGUARD=y

# Sbalance
CONFIG_IRQ_SBALANCE=y
CONFIG_SBALANCE_EXCLUDE_CPUS="3,7"

# Audio Power Amplifier
CONFIG_SND_SOC_AW87329_MI439=y
CONFIG_SND_SOC_AW87519_MI439=y

# Backlight
CONFIG_BACKLIGHT_CLASS_DEVICE=y
CONFIG_BACKLIGHT_KTD3136_MI439=y
CONFIG_MFD_TI_LMU_MI439=y

# Fingerprint
CONFIG_FPR_FPC=n
CONFIG_FPR_FPC_MI439=y
CONFIG_FINGERPRINT_GOODIX_GF3208_MI439=y

# Huaqin
CONFIG_HQ_SYSFS_SUPPORT_MI439=y

# Mach
CONFIG_MACH_XIAOMI_SDM439=y
CONFIG_MACH_XIAOMI_PINE=y
CONFIG_MACH_XIAOMI_OLIVE=y
CONFIG_MACH_XIAOMI_OLIVELITE=y
CONFIG_MACH_XIAOMI_OLIVEWOOD=y

# IR
CONFIG_LIRC=y
CONFIG_RC_CORE=y
CONFIG_RC_DECODERS=y
CONFIG_MEDIA_RC_SUPPORT=y
CONFIG_IR_SPI_MI439=y

# Touchscreen
CONFIG_INPUT_TOUCHSCREEN=y
CONFIG_TOUCHSCREEN_SYSCTL_MI439=y
CONFIG_TOUCHSCREEN_GOODIX_GT1X_MI439=y
CONFIG_TOUCHSCREEN_FT5446_MI439=y
CONFIG_TOUCHSCREEN_ITK9881H_MI439=y
CONFIG_TOUCHSCREEN_NT36525B_MI439=y
CONFIG_TOUCHSCREEN_FT8006S_MI439=y
